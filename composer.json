{
	"name": "kuriousagency/global-snippets",
	"description": "Create re-usable chunks of content for templates",
	"type": "craft-plugin",
	"version": "2.1.1",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"global snippets"
	],
	"support": {
		"docs": "https://github.com/KuriousAgency",
		"issues": "https://github.com/KuriousAgency"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Kurious Agency",
			"homepage": "https://kurious.agency"
		}
	],
	"require": {
		"craftcms/cms": "^3.0.0-RC1"
	},
	"autoload": {
		"psr-4": {
			"kuriousagency\\globalsnippets\\": "src/"
		}
	},
	"extra": {
		"name": "Global Snippets",
		"handle": "global-snippets",
		"hasCpSettings": true,
		"hasCpSection": true,
		"components": {
			"snippets": "kuriousagency\\globalsnippets\\services\\Snippets"
		},
		"class": "kuriousagency\\globalsnippets\\GlobalSnippets"
	}
}
